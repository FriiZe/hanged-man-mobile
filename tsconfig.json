{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "isolatedModules": true,
    "jsx": "react-native",
    "lib": [
      "dom",
      "esnext",
      "ES2015.Collection",
      "ES2015.Iterable"
    ],
    "moduleResolution": "node",
    "downlevelIteration": true,
    "noEmit": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "strict": true,
  },
}